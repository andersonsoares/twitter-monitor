<%= form_for @twitte, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :id_str, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :id_str, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :from_user, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :from_user, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :from_user_id_str, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :from_user_id_str, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :profile_image_url, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :profile_image_url, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :text, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :text, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :date, :class => 'control-label' %>
    <div class="controls">
      <%= f.datetime_select :date, :class => 'datetime_select' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :keyword_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :keyword_id, :class => 'number_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                keyword_twittes_path(@keyword.id), :class => 'btn' %>
  </div>
<% end %>
